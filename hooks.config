checks:
    - precommit-dummy
    - specification
    - branch-has-specification
    - allowed-project
    - branch-pattern
specification:
    format: 'jira'
branch-has-specification:
    exceptions:
        - "^release/"
allowed-project:
    allowed:
        - PF
        - CD
branch-pattern:
    allowed:
        - "^release/R"
        - "^feature/"